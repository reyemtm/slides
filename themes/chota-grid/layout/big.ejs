<!--layout: big.html-->
<script>
  var stylesheets = [
    "/assets/big-assets/big.css",
    "/assets/big-assets/highlight.css",
  ]

  function addStyles(stylesheets) {
    for (var s = 0; s < stylesheets.length; s++) {
      let link = document.createElement( 'link' );
      link.rel = 'stylesheet';
      link.type = 'text/css';
      link.href = stylesheets[s];
      document.getElementsByTagName( 'head' )[0].appendChild( link );
    }
  }

  addStyles(stylesheets)

</script>
<%- page.content %>
<script src='/assets/big-assets/big.js'></script>
<script src='/assets/big-assets/highlight.js'></script>
<script>hljs.initHighlightingOnLoad();</script>