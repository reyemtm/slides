<main class="container">
<div class="grid">
  <section>
    
    <blockquote>
      <%- config.description %>
    </blockquote>

  </section>
  <section>
  <% site.pages.sort('path').reverse().each(function(page,i) {%>
  <% if (page.layout == "slides-reveal" || page.layout == "slides-big") {%>
    <a href="<%- page.path %>"><h1><%- page.title %></h1></a>
  <%}%>
  <%})%>
  </section>
 </div>
</main>